{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-await-in-loop": "off",
        "import/extensions": "off",
        "quotes": "off",
        "import/no-named-as-default": "off",
        "import/no-named-as-default-member": "off"
    }
}
